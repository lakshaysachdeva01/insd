

        <% blogs.forEach(item => { %>
          <div onclick="window.location.href='/insight/<%= item.seoDetails.slug %>';">
            <article class="post type-post panel vstack gap-3 rounded-3 p-2 pb-3 bg-secondary dark:bg-gray-800">
                <a class='position-absolute top-0 ltr:start-0 rtl:end-0 m-3 fs-7 fw-bold text-none z-1 bg-primary text-white py-narrow px-1' href='/insight/<%= item.seoDetails.slug %>' style='border-radius: 8px'>Marketing</a>
                <figure class="featured-image m-0 rounded ratio ratio-3x2 rounded-2 uc-transition-toggle overflow-hidden">
                    <img class="media-cover image uc-transition-scale-up uc-transition-opaque" src="<%= process.env.S3_BASE_URL + item.banner.image %>" alt="Top 5 reasons to invest in marketing">
                    <a class='position-cover' data-caption='Top 5 reasons to invest in marketing' href='/insight/<%= item.seoDetails.slug %>'></a>
                </figure>
                <header class="panel vstack items-center gap-1 lg:gap-2 px-2">
                    <h3 class="h5 xl:h4 m-0 text-center m-0">
                        <a class='text-none' href='/insight/<%= item.seoDetails.slug %>'><%= item.title %></a>
                    </h3>
                    <ul class="post-meta nav-x ft-tertiary justify-center gap-1 fs-7 text-gray-400 dark:text-gray-300lg:d-flex">
                
                        <li>
                            <div class="post-date hstack gap-narrow">
                                <span><%= item.postDate %></span>
                            </div>
                        </li>
                    </ul>
                </header>
            </article>
        </div>

        <% }) %>

