            <script>
            // JavaScript to toggle logo based on sticky state
            document.addEventListener('DOMContentLoaded', function () {
                const navbar = document.querySelector('.uc-navbar-container');
                const logoImg = document.querySelector('.uc-logo .logo-img');
                
                if (navbar && logoImg) {
                    const defaultSrc = logoImg.getAttribute('data-default-src');
                    const stickySrc = logoImg.getAttribute('data-sticky-src');
                    
                    const observer = new MutationObserver(function (mutations) {
                        mutations.forEach(function (mutation) {
                            if (mutation.attributeName === 'class') {
                                if (navbar.classList.contains('uc-navbar-sticky')) {
                                    logoImg.src = stickySrc;
                                } else {
                                    logoImg.src = defaultSrc;
                                }
                            }
                        });
                    });
                    
                    observer.observe(navbar, { attributes: true });
                    
                    // Initial check in case navbar is sticky on load
                    if (navbar.classList.contains('uc-navbar-sticky')) {
                        logoImg.src = stickySrc;
                    }
                }
            });
            </script>